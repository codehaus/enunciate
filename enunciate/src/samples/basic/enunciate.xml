<?xml version="1.0"?>
<enunciate xmlns="http://enunciate.sf.net">
  <namespaces>
    <namespace id="pets" uri="http://net.sf.enunciate/samples/petclinic/pets"/>
    <namespace id="owners" uri="http://net.sf.enunciate/samples/petclinic/owners"/>
    <namespace id="vets" uri="http://net.sf.enunciate/samples/petclinic/vets"/>
  </namespaces>
  <modules>
    <xml>
      <schemas>
        <schema namespaceRef="pets" generate="true" file="pet.xsd" location="http://net.sf.enunciate/samples/schemas/pet.xsd"/>
      </schemas>
    </xml>
    <xfire-client>
      <client-package-conversions>
        <convert from="net.sf.enunciate.samples.petclinic" to="net.sf.enunciate.samples.petclinic.client"/>
        <convert from="net.sf.enunciate.samples.petclinic.services" to="net.sf.enunciate.samples.petclinic.client.services"/>
      </client-package-conversions>
    </xfire-client>
  </modules>
</enunciate>
