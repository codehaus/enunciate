<?xml version="1.0"?>
<enunciate label="full">
  <namespaces>
    <namespace id="data" uri="http://enunciate.codehaus.org/samples/genealogy/data"/>
    <namespace id="cite" uri="http://enunciate.codehaus.org/samples/genealogy/cite"/>
    <namespace id="full" uri="http://enunciate.codehaus.org/samples/full"/>
  </namespaces>
  <services>
    <soap defaultSubcontext="/soap-services/">
      <service name="source-service" relativePath="/soap-services/sources/source"/>
    </soap>
  </services>
  <modules>
    <xfire-client uuid="full.example">
      <package-conversions>
        <convert from="org.codehaus.enunciate.samples.genealogy" to="org.codehaus.enunciate.samples.genealogy.client"/>
      </package-conversions>
    </xfire-client>
    <xfire uuid="full.example">
      <springImport file="../custom_spring.xml"/>
      <copyResources dir="../../../../src/samples/full" pattern="**/*.txt"/>
      <globalServiceInterceptor beanName="globalServiceAdvice"/>
    </xfire>
    <docs splashPackage="org.codehaus.enunciate.samples.genealogy" title="MRCGS API" copyright="My Really Cool Genealogy Site">
      <download name="License" file="../dummy-license.txt" description="The license file governing these downloads."/>
    </docs>
  </modules>
</enunciate>
