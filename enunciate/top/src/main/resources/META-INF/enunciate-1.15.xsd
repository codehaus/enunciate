              <xs:attribute name="locale" type="xs:string"/>
              <xs:attribute name="optimize" type="xs:boolean"/>
              <xs:attribute name="debug" type="xs:boolean"/>
              <xs:attribute name="profile" type="xs:boolean"/>
              <xs:attribute name="strict" type="xs:boolean"/>
              <xs:attribute name="useNetwork" type="xs:boolean"/>
              <xs:attribute name="incremental" type="xs:boolean"/>
              <xs:attribute name="warnings" type="xs:boolean"/>
