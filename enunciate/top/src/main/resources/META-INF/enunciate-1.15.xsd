      <xs:extension base="module-base">
        <xs:sequence>
          <xs:element name="json" minOccurs="0" maxOccurs="1">
            <xs:annotation><xs:documentation>supports configuration options for JSON serialization</xs:documentation></xs:annotation>
            <xs:complexType>
              <xs:attribute name="defaultJsonSerialization">
                <xs:annotation><xs:documentation>used to define the default default JSON serialization method.</xs:documentation></xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="hierarchical"/>
                    <xs:enumeration value="xmlMapped"/>
                    <xs:enumeration value="badgerfish"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
              <xs:attribute name="xstreamReferenceAction">
                <xs:annotation><xs:documentation>used to define the action that XStream takes with object references</xs:documentation></xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="relative_references"/>
                    <xs:enumeration value="id_references"/>
                    <xs:enumeration value="no_references"/>
                    <xs:enumeration value="absolute_references"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:complexType>
          </xs:element>
          <xs:element name="content-type-handlers" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="handler" minOccurs="0" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:attribute name="contentType" type="xs:string"/>
                    <xs:attribute name="class" type="xs:string"/>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="defaultContentTypeHandler" type="xs:string" default="org.codehaus.enunciate.modules.rest.xml.JaxbXmlContentHandler">
          <xs:annotation><xs:documentation>used to define the default content type handler when no others are found for a given content type.</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="requireJAXBCompatibility" type="xs:boolean" default="true">
          <xs:annotation><xs:documentation>used to disable the requirement that return types and noun values be JAXB-compatible. Since Enunciate uses JAXB by default for marshalling and umarshalling, this requirement should only be disabled if you have provided your own content type handlers.</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="handlerExceptionResolver" type="xs:string" default="org.codehaus.enunciate.modules.rest.RESTResourceExceptionHandler">
          <xs:annotation><xs:documentation>Used to set the exception handler responsible for handling exceptions that arise within the course of running the resource</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="conversionSupport" type="xs:string" default="org.codehaus.enunciate.modules.rest.DefaultConverter">
          <xs:annotation><xs:documentation>The converter to use when populating method parameters from request parameters</xs:documentation></xs:annotation>
        </xs:attribute>                
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="module-jersey">
    <xs:annotation><xs:documentation>Configuration for the Jersey module</xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="module-base">
        <xs:sequence>
          <xs:element name="init-param" minOccurs="0" maxOccurs="unbounded">
